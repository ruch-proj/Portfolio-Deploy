{
  "functions": {
    "api/index.py": {
      "runtime": "python@3.9"
    }
  },
  "builds": [
    {
      "src": "portfolio.py",
      "use": "@vercel/python",
      "config": {
        "zeroConfig": true
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api"
    }
  ]
}
